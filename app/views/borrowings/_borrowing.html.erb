<div id="<%= dom_id borrowing %>">
  <p class="my-5">
    <strong class="block mb-1 font-medium">Book:</strong>
    <%= borrowing.book_id %>
  </p>

  <p class="my-5">
    <strong class="block mb-1 font-medium">Borrower:</strong>
    <%= borrowing.borrower.name %>
  </p>

  <p class="my-5">
    <strong class="block mb-1 font-medium">Borrowed on:</strong>
    <%= borrowing.borrowed_on %>
  </p>

  <p class="my-5">
    <strong class="block mb-1 font-medium">Returned on:</strong>
    <%= borrowing.returned_on %>
  </p>

</div>
