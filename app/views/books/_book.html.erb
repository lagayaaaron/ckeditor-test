<div id="<%= dom_id book %>">
  <p class="my-5">
    <strong class="block mb-1 font-medium">Title:</strong>
    <%= book.title %>
  </p>

  <p class="my-5">
    <strong class="block mb-1 font-medium">Published year:</strong>
    <%= book.published_year %>
  </p>

  <p class="my-5">
    <strong class="block mb-1 font-medium">Author:</strong>
    <%= book.author.name %>
  </p>

</div>
